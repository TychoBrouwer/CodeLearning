{
    "manifest_version": 1,
    "type": "WebApp",
    "meta": {
        "name": "StatsDB",
        "author": "StatsDB",
        "version": "1.4.4.0",
        "minimum-overwolf-version": "0.155.83.11",
        "minimum-gep-version": "0.162.0.0",
        "description": "StatsDB Stats Tracker, stay one step ahead",
        "dock_button_title": "StatsDB",
        "icon": "icons/IconMouseOver.png",
        "icon_gray": "icons/IconMouseNormal.png",
        "launcher_icon": "icons/icon.ico",
        "window_icon": "icons/TaskbarIcon.png"
    },
    "permissions": [
        "Extensions",
        "Streaming",
        "Profile",
        "Hotkeys",
        "GameInfo",
        "Tray",
        "DesktopStreaming"
    ],
    "data": {
        "start_window": "background",
        "windows": {
            "background": {
                "file": "dist/background/background.html",
                "is_background_page": true
