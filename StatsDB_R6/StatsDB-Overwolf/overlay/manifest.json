        "game_targeting": {
            "type": "dedicated",
            "game_ids": [
                10826
            ]
        },
        "game_events": [
            10826
        ],
        "launch_events": [{
            "event": "GameLaunch",
            "event_data": {
                "game_ids": [
                    10826
                ]
            },
            "start_minimized": true
        }],
        "hotkeys": {
            "showhide": {
                "title": "Show/Hide R6Lookup",
                "action-type": "toggle",
                "default": "Ctrl+X"
            }
        },
        "protocol_override_domains": {
            "googlesyndication": "http"
        },
        "externally_connectable": {
            "matches": [
                "http://*.overwolf.com",
                "https://*.overwolf.com",
                "https://*.r6db.net",
                "https://ubisoft-avatars.akamaized.net"
            ]
        },
        "force_browser": "user",
        "developer": {
            "enable_auto_refresh": true,
            "reload_delay": 500,
            "filter": "*.json;*.html;*.js;*.css;*.png;*.jpg;*.webp;*.svg;*.ico"
        },
        "disable_cleanup": true,
        "process_name": "StatsDB Overlay"
    }
