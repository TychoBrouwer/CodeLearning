            "desktop": {
                "file": "dist/desktop/desktop.html",
                "desktop_only": true,
                "show_in_taskbar": true,
                "native_window": true,
                "resizable": false,
                "use_os_windowing": true,
                "transparent": true,
                "keep_window_location": true,
                "block_top_window_navigation": true,
                "popup_blocker": true,
                "mute": true,
                "size": {
                    "width": 900,
                    "height": 600
                }
            },
            "in_game": {
                "file": "dist/in_game/in_game.html",
                "in_game_only": true,
                "block_top_window_navigation": true,
                "popup_blocker": true,
