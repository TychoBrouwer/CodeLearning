{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "code": 0,
        "max-len": 0,
        "quotes": [
            2,
            "single"
        ],
        "indent": [
            "error",
            4
        ]
    }
}